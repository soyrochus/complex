# Complex DSL Environment Configuration
# Copy this file to .env and update with your database settings

# PostgreSQL Database Configuration
COMPLEX_DB_HOST=localhost
COMPLEX_DB_PORT=5432
COMPLEX_DB_NAME=postgres
COMPLEX_DB_USER=postgres
COMPLEX_DB_PASSWORD=your_password_here

# Apache AGE Graph Configuration
COMPLEX_GRAPH_NAME=complex_graph

# Reference Strategy Configuration
# Set to 'true' for edge-mapped references, 'false' for scalar-ID references
EDGE_REFERENCES=true

# Optional: Additional PostgreSQL connection parameters
# COMPLEX_DB_SSLMODE=prefer
# COMPLEX_DB_CONNECT_TIMEOUT=10
# COMPLEX_DB_APPLICATION_NAME=complex_dsl
